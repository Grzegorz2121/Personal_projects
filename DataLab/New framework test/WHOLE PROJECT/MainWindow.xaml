<Window x:Name="MainWindow1" x:Class="New_framework_test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:New_framework_test"
        mc:Ignorable="d"
        Title="DataLab" Height="450" Width="800">
    <Grid x:Name="MainGrid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="Add" Content="Add component" HorizontalAlignment="Left" Height="40" VerticalAlignment="Top" Width="108" Click="Button_Click_1" Margin="10,10,0,0"/>
        <ListBox x:Name="block_listbox1" HorizontalAlignment="Left" Height="100" Margin="18,69,0,0" VerticalAlignment="Top" Width="100" SelectionChanged="block_listbox1_SelectionChanged"/>
        <Button x:Name="DEBUG" Content="Debug(obj list)" HorizontalAlignment="Left" Height="40" VerticalAlignment="Top" Width="108" Margin="18,199,0,0" Click="DEBUG_Click"/>
    </Grid>
</Window>
